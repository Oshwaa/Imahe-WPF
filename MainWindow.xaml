<Window x:Class="Imahe.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Imahe"
        xmlns:userControls="clr-namespace:Imahe.views.UserControls"
        mc:Ignorable="d"
        MinWidth="1200"
        MinHeight="700"
        
        Title="Imahe" Height="700" Width="1200" WindowStyle="ThreeDBorderWindow" Icon="/assets/images/icon.png">
    <Grid Background="#000000">
        <Grid.ColumnDefinitions>
            <ColumnDefinition Width="18*"/>
            <ColumnDefinition Width="132*"/>
            <ColumnDefinition Width="43*"/>
            <ColumnDefinition Width="317*"/>
            <ColumnDefinition Width="90*"/>
        </Grid.ColumnDefinitions>
        <userControls:sidePanel Grid.Column="0" Background="GhostWhite" Grid.ColumnSpan="2"/>

    </Grid>




</Window>
